@model Category
<div class="text-center">
    <h1>@Model.Name</h1>
</div>
<div class="content">
    <div class="left mr-5">
        <h3>Categories</h3>
        @{
            foreach (var category in ViewBag.ProductInCategories)
            {
                <p>- @category.Product.Name</p>
            }
        }
    </div>
    @{
        Association b = new Association
        {
            CatagoryId = Model.CatagoryId
        };
    }
    <partial name="_AddCatAssociations" model="@b"></partial>
</div>